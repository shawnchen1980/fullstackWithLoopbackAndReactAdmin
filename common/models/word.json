{
  "name": "Word",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Commons": true
  },
  "properties": {
    "spelling": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "wordbooks": {
      "type": "hasMany",
      "model": "Wordbook",
      "foreignKey": "wordId",
      "through": "WordMapping",
      "keyThrough": "bookId"
    },
    "owner": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "ownerId"
    }
  },
  "acls": [],
  "methods": {}
}
